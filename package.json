{
    "name": "socialcolander.github.io",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "run-s clear dev",
        "dev": "run-p jekyll-dev backend live-server scripts-dev",
        "prod": "run-s clear scripts-prod jekyll-prod",
        "clear": "/bin/rm -rf dist/*",
        "jekyll-dev": "jekyll build -w --config '_config.yml,_config.dev.yml'",
        "jekyll-prod": "jekyll build --config '_config.yml,_config.prod.yml'",
        "live-server": "cd ./dist && live-server --port=2000",
        "backend": "chmod -R 777 ./_server && run-p backend-server backend-crawler",
        "backend-server": "cd ./_server && ./backend -c ./config.json -l ./",
        "backend-crawler": "cd ./_server && ./crawler -c ./config.json -l ./",
        "webpack": "webpack --config ./webpack.config.js --display-error-details",
        "scripts-dev": "npm run webpack -- -w -d",
        "scripts-prod": "npm run webpack -- -p"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/socialcolander/socialcolander.github.io.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/socialcolander/socialcolander.github.io/issues"
    },
    "homepage": "https://github.com/socialcolander/socialcolander.github.io#readme",
    "devDependencies": {
        "babel": "^6.23.0",
        "babel-core": "^6.24.0",
        "babel-loader": "^6.4.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.0",
        "babel-preset-stage-0": "^6.22.0",
        "css-loader": "^0.28.0",
        "npm-run-all": "^4.0.2",
        "style-loader": "^0.16.1",
        "webpack": "^2.3.2",
        "webpack2-polyfill-plugin": "^0.0.2"
    }
}
